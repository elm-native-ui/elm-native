{
  "name": "ElmNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precompile": "rm -f ._eml.js && bash ./scripts/patch-vdom.sh",
    "compile": "elm-make Main.elm --output ._elm.js",
    "postcompile": "echo 'module.exports = Elm;' >> ._elm.js && mv ._elm.js elm.js",
    "start": "chokidar '**/*.elm' -c 'npm run compile'"
  },
  "dependencies": {
    "chokidar-cli": "^1.2.0",
    "react-native": "0.21.0-rc"
  }
}
