{
  "name": "ElmNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precompile": "rm -f elm.js",
    "compile": "elm-make Main.elm --output elm.js",
    "postcompile": "echo 'module.exports = Elm;' >> elm.js && perl -p -i -e 's/\\brequire\\b/_browserify_require/g' elm.js",
    "start": "chokidar '**/*.elm' -c 'npm run compile'"
  },
  "dependencies": {
    "chokidar-cli": "^1.2.0",
    "react-native": "0.21.0-rc"
  }
}
